<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="576.0" prefWidth="1024.0" styleClass="body" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sudoku.LandingScreen">
   <children>
      <ImageView fitHeight="261.0" fitWidth="647.0" layoutX="189.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@solverLogo.png" />
         </image>
      </ImageView>
      <Button layoutX="540.0" layoutY="435.0" mnemonicParsing="false" onAction="#startSolver" styleClass="btn" text="START">
         <font>
            <Font name="Segoe UI" size="25.0" />
         </font>
      </Button>
      <Button layoutX="395.0" layoutY="435.0" mnemonicParsing="false" onAction="#quitSolver" styleClass="btn" text="QUIT">
         <font>
            <Font name="Segoe UI" size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
