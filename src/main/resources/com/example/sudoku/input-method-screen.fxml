<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="576.0" prefWidth="1024.0" styleClass="body" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sudoku.InputMethodScreen">
   <children>
      <Pane fx:id="manualPane" layoutX="222.0" layoutY="115.0" onMouseClicked="#selectManual" prefHeight="323.0" prefWidth="235.0" styleClass="selectPane">
         <children>
            <ImageView fitHeight="179.0" fitWidth="169.0" layoutX="24.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@manual_input.png" />
               </image>
            </ImageView>
            <Text layoutX="33.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="txt" text="MANUALLY">
               <font>
                  <Font name="Segoe UI" size="33.0" />
               </font>
            </Text>
         </children>
      </Pane>
      <Pane fx:id="importPane" layoutX="567.0" layoutY="115.0" onMouseClicked="#selectImport" prefHeight="323.0" prefWidth="235.0" styleClass="selectPane">
         <children>
            <ImageView fitHeight="199.0" fitWidth="152.0" layoutX="42.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@import_input.png" />
               </image>
            </ImageView>
            <Text layoutX="58.0" layoutY="275.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="txt" text="IMPORT">
               <font>
                  <Font name="Segoe UI" size="33.0" />
               </font>
            </Text>
         </children>
      </Pane>
      <Text layoutX="115.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="txt" text="HOW DO YOU WANT TO INPUT THE SUDOKU ?">
         <font>
            <Font name="Segoe UI" size="38.0" />
         </font>
      </Text>
      <Button layoutX="434.0" layoutY="483.0" mnemonicParsing="false" onAction="#inputContinue" styleClass="btn" text="CONTINUE">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
